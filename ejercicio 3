int rojoPin=13;
 int azulPin=12;
 int verdePin=9;
 
void setup (){
  pinMode(rojoPin,OUTPUT);
  pinMode(azulPin,OUTPUT);
  pinMode(verdePin,OUTPUT);

}
void loop(){
  digitalWrite(rojoPin,HIGH);
  delay(100);
   digitalWrite(rojoPin,HIGH);
  delay(100);
  
  digitalWrite(azulPin,HIGH);
  delay(100);
  digitalWrite(azulPin,HIGH);
  delay(100); 
  
  digitalWrite(verdePin,HIGH);
  delay(100);
  digitalWrite(verdePin,HIGH);
  delay(100);
}
