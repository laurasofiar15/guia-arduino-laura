#include <LiquidCrystal.h>
#include <DHT11.h>

int sensor =2;
int temp, humedad ;
int dt=500;

 DHT11 (sensor,DHT11);

 void setup(){
  Serial.begin(9600);
  dht11.begin ();

  }
  void loop (){
   humedad = dht11.readHumidity();
   temp= dht11.readTemperature();
    
  }
Serial.print ("Temperatura:");
Serial.print(temp);
Serial.print ("Â°C humedad:");
Serial.print("%");
delay(dt);
